{
  "name": "@nuxt/ui-pro",
  "description": "Ship beautiful web applications at the speed of light with Nuxt UI Pro.",
  "type": "module",
  "version": "1.5.0",
  "packageManager": "pnpm@9.12.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/ui-pro.git"
  },
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "lib",
    "modules",
    "plugins",
    "types",
    "app.config.ts",
    "nuxt.config.ts",
    "tailwind.config.ts"
  ],
  "theme": {
    "env": "NUXT_UI_PRO_LICENSE",
    "link": "https://ui.nuxt.com/pro/pricing"
  },
  "scripts": {
    "dev": "DEV=true nuxi dev .docs",
    "build": "DEV=true nuxi generate .docs",
    "play": "nuxi dev .playground",
    "prepare": "DEV=true nuxi prepare .docs",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "release": "release-it",
    "test": "TZ=Europe/Paris vitest"
  },
  "dependencies": {
    "@iconify-json/vscode-icons": "^1.2.2",
    "@nuxt/ui": "^2.19.2",
    "@vueuse/core": "^11.2.0",
    "defu": "^6.1.4",
    "git-url-parse": "^15.0.0",
    "ofetch": "^1.4.1",
    "parse-git-config": "^3.0.0",
    "pathe": "^1.1.2",
    "pkg-types": "^1.2.1",
    "tailwind-merge": "^2.5.4",
    "vue3-smooth-dnd": "^0.0.6"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint-config": "^0.6.1",
    "@nuxt/image": "^1.8.1",
    "@nuxt/test-utils": "^3.14.4",
    "@release-it/conventional-changelog": "^9.0.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.14.0",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.14.0",
    "release-it": "^17.10.0",
    "vitest": "^2.1.4",
    "vitest-environment-nuxt": "^1.0.1",
    "vue-tsc": "^2.1.10",
    "zod": "^3.23.8"
  },
  "resolutions": {
    "@nuxt/ui-pro": "workspace:*"
  }
}
